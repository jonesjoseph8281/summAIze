{
  "manifest_version": 3,
  "name": "Text Assistant",
  "version": "1.0",
  "description": "Floating buttons for selected text",
  "permissions": [],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["contentScript.js"]
    }
  ],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png"
  }
}